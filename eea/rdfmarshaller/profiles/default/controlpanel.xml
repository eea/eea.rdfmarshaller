<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="plone">

  <configlet
    title="Define Copyright License"
    action_id="rdfmarshaller_licenses"
    appId="rdfmarshaller"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@licenses-settings"
    icon_expr=""
    visible="True"
    i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>

  <configlet
    title="Assign Licenses to Portal Types"
    action_id="rdfmarshaller_portal_type_licenses"
    appId="rdfmarshaller"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@portal-type-licenses-settings"
    icon_expr=""
    visible="True"
    i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>

  <configlet
    title="Manage LinkedData Homepages"
    action_id="rdfmarshaller_linkeddata"
    appId="rdfmarshaller"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@manage-linkeddata"
    icon_expr=""
    visible="True"
    i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
